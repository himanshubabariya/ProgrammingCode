@model List<ProgrammingCode.DAL.PRO.PRO_ProgramTestCase.SelectForSearch_Result>
@using System.Net;
@using Microsoft.AspNetCore.Html
<div class="card  shadow-sm mt-5">
    <div class="card-header">
        <h3 class="card-title ">
            Search Result
          </h3>
        <div class="card-toolbar">


            <a class="btn btn-sm btn-success js-stkModal-btn ms-1"
               asp-area="PRO_ProgramTestCase" asp-action="AddEdit" asp-controller="PRO_ProgramTestCase">
                <i class="la la-plus"></i> Add New
            </a>
        </div>
    </div>
    <div class="card-body">
        <div class="table-responsive">
            <table class="table table-hover" id="tblList" cellspacing="0" width="100%" style="border: 1px solid #ccc;">
            <thead class="bg-gray-300 ">
                <tr>
                    <th class="text-center" style="border: 1px solid #ccc;">Actions</th>
                    
      
                    <th class="text-center" style="border: 1px solid #ccc;">defination</th>
                    <th class="text-center" style="border: 1px solid #ccc;">Type</th>
                    <th class="text-center" style="border: 1px solid #ccc;">Sequence</th>
                        <td class="text-center" style="border: 1px solid #ccc;">Created User</td>
                    <th class="text-center" style="border: 1px solid #ccc;">Modified</th>
                  
                    
                </tr>
            </thead>
                <tbody>
                @{
                    int i = 0;
                }
                @foreach (var item in Model)
                {
                    i++;
                    <tr>

                        <td class="text-center" style="border: 1px solid #ccc;">
                            <span data-bs-toggle="tooltip" data-bs-placement="left" title="Edit">
                                <a class="btn btn-primary btn-icon btn-elevate btn-sm js-btn"
                               asp-route-ProgramTestCaseID="@item.ProgramTestCaseID" asp-area="PRO_ProgramTestCase" asp-action="AddEdit" asp-controller="PRO_ProgramTestCase">
                                    <i class="la la-edit"></i>
                                    <input type="hidden" name="ProgramTestCaseID" value="@item.ProgramTestCaseID" />
                                </a>
                            </span>

                            <form id="formDelete_UMS" data-ajax="true" data-ajax-method="post" data-ajax-mode="replace" data-ajax-update="#SearchResult"
                                  data-ajax-success="OnSuccess(this.id,data, status, xhr);" data-ajax-failure="OnFailure(xhr, status, error);"
                              asp-area="PRO_ProgramTestCase" asp-action="_Delete" asp-controller="PRO_ProgramTestCase" style="display: inline;">
                                @Html.AntiForgeryToken()
                                <input type="hidden" name="ProgramTestCaseID" value="@item.ProgramTestCaseID" />
                                <a class="btn btn-danger btn-icon btn-elevate btn-sm js-bb-confirm-delete" data-bs-toggle="tooltip" data-bs-placement="left" title="Delete"><i class="la la-trash"></i></a>
                            </form>
                        </td>
                        <td class="" style="border: 1px solid #ccc;">@Html.Raw(WebUtility.HtmlDecode(item.Defination))</td>
                        <td class="text-center" style="border: 1px solid #ccc;">@item.IsPositive</td>
                        <td class="text-center" style="border: 1px solid #ccc;">@item.Sequence</td>
                            <td class="text-center" style="border: 1px solid #ccc;">@item.UserName</td>
                            <td class="text-center" style="border: 1px solid #ccc;">@item.Modified</td>
                       
                    </tr>
                }
            </tbody>
        </table>
        </div>
        <div class="col-md-12" style="text-align:center">
            <partial name="/Areas/PRO_ProgramTestCase/Views/PRO_ProgramTestCase/SelectForPage.cshtml" />
        </div>
    </div>
</div>